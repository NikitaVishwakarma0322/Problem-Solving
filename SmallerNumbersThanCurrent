class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
       int count1;
        int temp[] = new int[nums.length];
        for(int i=0 ; i<nums.length ; i++){
            count1 =0;
            for(int j=0;j<nums.length ; j++){
                if(nums[i]>nums[j]){
                    count1 = count1 + 1 ;
                }
            temp[i] = count1;
            }
        }
        return temp;
    }
}
